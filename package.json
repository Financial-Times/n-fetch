{
  "name": "@financial-times/n-fetch",
  "version": "0.0.0",
  "description": "The fetch logic we wanted",
  "repository": "financial-times/n-fetch",
  "main": "fetch.js",
  "license": "ISC",
  "dependencies": {
    "@financial-times/n-logger": "^8.0.0",
    "http-errors": "^1.8.1",
    "node-fetch": "^2.0.0"
  },
  "devDependencies": {
    "@financial-times/n-gage": "^8.3.2",
    "@financial-times/next-secrets": "^1.1.0",
    "chai": "^4.1.0",
    "check-engine": "^1.10.1",
    "eslint": "^4.18.0",
    "lintspaces-cli": "^0.6.0",
    "mocha": "^5.0.0",
    "nock": "^9.2.3",
    "proxyquire": "^1.7.11",
    "sinon": "^4.4.0",
    "snyk": "^1.167.2"
  },
  "scripts": {
    "prepare": "npx snyk protect || npx snyk protect -d || true",
    "preinstall": "[ \"$INIT_CWD\" != \"$PWD\" ] || npm_config_yes=true npx check-engine"
  },
  "engines": {
    "node": "14.x || 16.x",
    "npm": "7.x || 8.x"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node_modules/.bin/secret-squirrel-commitmsg",
      "pre-commit": "make verify && node_modules/.bin/secret-squirrel",
      "pre-push": "make unit-test && make verify -j3"
    }
  },
  "volta": {
    "node": "16.14.1",
    "npm": "7.20.2"
  }
}
